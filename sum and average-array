#include <iostream>
#include <iomanip>
using namespace std;

int main() {
    
    double M[12][12];
    double soma = 0.0;
    double media;
    int C;
    char T;
    
    cin >> C;
    cin >> T;
    
    for(int i = 0; i < 12; i++){
        for(int j = 0; j < 12; j++){
            cin >> M[i][j];
        }
    }
    
    for(int i = 0; i < 12; i++){
        soma = soma + M[i][C];  
    }
    
    media = soma / 12;
    
    cout << fixed << setprecision(1);
    
    if(T == 'M'){
        cout << media << endl;
    }
    else
        cout << soma << endl;
    
    return 0;
}
